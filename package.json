{
  "name": "snakeviz",
	"repository": {
		"url": "https://github.com/tillahoffmann/vscode-snakeviz"
	},
	"publisher": "tillahoffmann",
	"displayName": "Snakeviz",
	"description": "Visualize python profiling results using snakeviz.",
	"icon": "icon.png",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:snakeviz.snakeviz"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "snakeviz.snakeviz",
				"displayName": "Snakeviz",
				"selector": [
					{
						"filenamePattern": "*.prof"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	},
	"extensionDependencies": [
		"ms-python.python"
	],
	"dependencies": {
		"@vscode/python-extension": "^1.0.2"
	}
}
